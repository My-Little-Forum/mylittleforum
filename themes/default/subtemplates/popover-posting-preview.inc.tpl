 <template id="ajax-preview-template">
  <div id="ajax-preview" popover>
   <header>
    <button id="ajax-preview-close">
     <img class="sa-icon" src="themes/default/images/close-button.svg" alt="{#close#}" width="14" height="14">
    </button>
   </header>
   <div id="ajax-preview-content">
   </div>
   <footer>
    <a id="preview-open-post" href="">{#open_in_main_window#}</a>
    <a id="preview-reply" href="">{#reply_link#}</a>
   </footer>
  </div>
 </template>